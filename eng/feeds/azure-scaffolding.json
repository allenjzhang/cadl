{
  "azure-core": {
    "title": "Azure Data Plane Service",
    "description": "Create a project representing an Azure service Data Plane API",
    "libraries": ["@azure-tools/cadl-autorest"],
    "skipCompilerPackage": true,
    "config": {},
    "files": [
      {
        "path": "./data-plane/main.cadl",
        "destination": "main.cadl"
      },
      {
        "path": "./data-plane/client.cadl",
        "destination": "client.cadl"
      },
      {
        "path": "./data-plane/cadl-project.yaml",
        "destination": "cadl-project.yaml"
      },
      {
        "path": "./data-plane/examples/2022-11-01-preview/Widgets_CreateOrUpdateWidgetSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_CreateOrUpdateWidgetSample.json"
      },
      {
        "path": "./data-plane/examples/2022-11-01-preview/Widgets_DeleteWidgetSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_DeleteWidgetSample.json"
      },
      {
        "path": "./data-plane/examples/2022-11-01-preview/Widgets_GetWidgetOperationStatusSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_GetWidgetOperationStatusSample.json"
      },
      {
        "path": "./data-plane/examples/2022-11-01-preview/Widgets_GetWidgetSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_GetWidgetSample.json"
      },
      {
        "path": "./data-plane/examples/2022-11-01-preview/Widgets_ListWidgetsSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_ListWidgetsSample.json"
      }
    ],
    "inputs": {
      "ServiceNamespace": {
        "description": "Please provide service namespace in Pascal case:",
        "type": "text",
        "initialValue": "Azure.Contoso"
      }
    }
  },
  "azure-arm": {
    "title": "Azure Resource Manager Service",
    "description": "Create a project representing an Azure service ARM API",
    "libraries": [
      "@azure-tools/cadl-autorest",
      "@azure-tools/cadl-azure-resource-manager"
    ],
    "skipCompilerPackage": true,
    "config": {
      "emit": ["@azure-tools/cadl-autorest"]
    },
    "files": [
      {
        "path": "./resource-manager/main.cadl",
        "destination": "main.cadl"
      },
      {
        "path": "./resource-manager/client.cadl",
        "destination": "client.cadl"
      },
      {
        "path": "./resource-manager/cadl-project.yaml",
        "destination": "cadl-project.yaml"
      },
      {
        "path": "./resource-manager/examples/2022-11-01-preview/Widgets_CreateOrUpdateWidgetSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_CreateOrUpdateWidgetSample.json"
      },
      {
        "path": "./resource-manager/examples/2022-11-01-preview/Widgets_DeleteWidgetSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_DeleteWidgetSample.json"
      },
      {
        "path": "./resource-manager/examples/2022-11-01-preview/Widgets_GetWidgetOperationStatusSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_GetWidgetOperationStatusSample.json"
      },
      {
        "path": "./resource-manager/examples/2022-11-01-preview/Widgets_GetWidgetSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_GetWidgetSample.json"
      },
      {
        "path": "./resource-manager/examples/2022-11-01-preview/Widgets_ListWidgetsSample.json",
        "destination": "examples/2022-11-01-preview/Widgets_ListWidgetsSample.json"
      }
    ]
  }
}
